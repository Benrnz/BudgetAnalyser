                                 StrokeThickness="4"
                                 VerticalAlignment="Center"
                                 Width="40" />
                        <TextBlock Foreground="{StaticResource ButtonGlyphBrush}"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="24"
                                   Margin="-1,-1,0,0"
                                   FontWeight="Bold"
                                   Text="..." />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ToolTip"
                Value="Build" />
    </Style>

    <Style x:Key="Button.Round.Clear"
           BasedOn="{StaticResource Button.Round.Base}"
           TargetType="{x:Type Button}">
        <Setter Property="Template">
            <Setter.Value>

                <ControlTemplate>
                    <Grid Height="40"
                          Width="40">
                        <Ellipse Fill="{TemplateBinding Background}"
                                 Height="40"
                                 HorizontalAlignment="Center"
                                 Stroke="{StaticResource ButtonStrokeBrush}"
                                 StrokeThickness="4"
                                 VerticalAlignment="Center"
                                 Width="40" />
                        <Ellipse Height="15"
                                 Width="15"
                                 HorizontalAlignment="Center"
                                 VerticalAlignment="Center"
                                 Stroke="{StaticResource ButtonGlyphBrush}"
                                 StrokeThickness="2" />
                        <Path Data="M19,0 L0,16"
                              HorizontalAlignment="Center"
                              Margin="12.5,14.75,12.5,15"
                              StrokeThickness="2"
                              Stroke="{StaticResource ButtonGlyphBrush}"
                              VerticalAlignment="Center"
                              Height="10.25"
                              Stretch="Fill"
                              Width="15" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ToolTip"
                Value="Clear all filters" />
    </Style>

    <Style x:Key="Button.Round.Cross"
           BasedOn="{StaticResource Button.Round.Base}"
           TargetType="{x:Type Button}">
        <Setter Property="Template">
            <Setter.Value>

                <ControlTemplate>
                    <Grid Height="40"
                          Width="40">
                        <Ellipse Fill="{TemplateBinding Background}"
                                 Height="40"
                                 HorizontalAlignment="Center"
                                 Stroke="{StaticResource ButtonStrokeBrush}"
                                 StrokeThickness="4"
                                 VerticalAlignment="Center"
                                 Width="40" />
                        <Path Data="M0,0 L1,1 M0,1 L1,0"
                              Height="20"
                              HorizontalAlignment="Center"
                              Stretch="Uniform"
                              Stroke="{StaticResource ButtonGlyphBrush}"
                              StrokeThickness="3"
                              VerticalAlignment="Center"
                              Width="20" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ToolTip"
                Value="Close" />
        <Setter Property="IsCancel"
                Value="True" />
    </Style>

   <Style x:Key="Button.Round.SmallCross"
           BasedOn="{StaticResource Button.Round.Cross}"
           TargetType="Button">
        <Setter Property="HorizontalAlignment"
                Value="Center" />
        <Setter Property="VerticalAlignment"
                Value="Center" />
        <Setter Property="ToolTip"
                Value="Remove" />
        <Setter Property="LayoutTransform">
            <Setter.Value>
                <ScaleTransform CenterX="0.5"
                                CenterY="0.5"
                                ScaleX="0.5"
                                ScaleY="0.5" />
