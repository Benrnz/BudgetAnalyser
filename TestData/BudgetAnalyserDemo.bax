<?xml version="1.0" encoding="utf-8"?>
<BudgetAnalyserStorageRoot xmlns="clr-namespace:BudgetAnalyser.Engine.Persistence;assembly=BudgetAnalyser.Engine"
                           xmlns:scg="clr-namespace:System.Collections.Generic;assembly=mscorlib"
                           xmlns:beld="clr-namespace:BudgetAnalyser.Engine.Ledger.Data;assembly=BudgetAnalyser.Engine"
						               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <BudgetAnalyserStorageRoot.MatchingRulesCollectionRootDto>
    <StorageBranch Source="DemoMatchingRules.xml" />
  </BudgetAnalyserStorageRoot.MatchingRulesCollectionRootDto>

  <BudgetAnalyserStorageRoot.LedgerBookRootDto>
    <StorageBranch Source="DemoLedgerBook.xml" />
  </BudgetAnalyserStorageRoot.LedgerBookRootDto>

  <BudgetAnalyserStorageRoot.StatementModelRootDto>
    <StorageBranch Source="DemoTransactions.csv" />
  </BudgetAnalyserStorageRoot.StatementModelRootDto>

  <BudgetAnalyserStorageRoot.BudgetCollectionRootDto>
    <StorageBranch Source="DemoBudget.xml" />
  </BudgetAnalyserStorageRoot.BudgetCollectionRootDto>

  <BudgetAnalyserStorageRoot.LedgerReconciliationToDoCollection>
    <scg:List x:TypeArguments="beld:ToDoTaskDto" Capacity="4">
      <beld:ToDoTaskDto CanDelete="False" Description="Save for 3 Mtg Pmts in May" SystemGenerated="False" />
      <beld:ToDoTaskDto CanDelete="False" Description="Transfer any excess to On Call (~>1000)" SystemGenerated="False" />
    </scg:List>
  </BudgetAnalyserStorageRoot.LedgerReconciliationToDoCollection>

  <BudgetAnalyserStorageRoot.WidgetCollectionRootDto>
    <StorageBranch Source="Widgets.xml" />
  </BudgetAnalyserStorageRoot.WidgetCollectionRootDto>
</BudgetAnalyserStorageRoot>
